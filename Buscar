def horizontal():
    repeat 9:
        move()
        if object_here():
            take()
        
   
def vertical():
    turn_left()
    horizontal()
        
def turnRight():
    repeat 3:
        turn_left()
        
def giroIzq():
    turn_left()
    move()
    turn_left()
    horizontal()
    
def giroDer():
    turnRight()
    move()
    turnRight()
    horizontal()
    
def zig_zag2():
    vertical()
    giroDer()
    giroIzq()
    giroDer()
    giroIzq()
    giroDer()
    giroIzq()
    giroDer()
    giroIzq()
    giroDer()
    
    
def buscar():
    zig_zag2()
        
        

buscar()

